<tau:Window x:Class="Tauron.Application.ServiceManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tauron.Application.ServiceManager"
        xmlns:tau="http://schemas.TauronWpf.com"
        xmlns:sync="http://schemas.syncfusion.com/wpf"
        mc:Ignorable="d"
        Title="{tau:Loc MainWindow_Label_Title}" Height="450" Width="800">
    <sync:DockingManager CanClose="False" UseDocumentContainer="True" DockAbility="All">
        <ContentControl sync:DockingManager.Header="{tau:Loc MainWindow_DockHeader_NodeView}" sync:DockingManager.State="Document"></ContentControl>
    </sync:DockingManager>
</tau:Window>
