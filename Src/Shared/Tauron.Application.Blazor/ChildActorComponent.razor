@typeparam TModelType
@using Tauron.Application.CommonUI
@inherits ActorComponentBase<TModelType>

@ChildContent

@code {

    [Parameter]
    public RenderFragment? ChildContent { get; set; }

    [CascadingParameter]
    public IViewModel<TModelType>? RootModel { get; set; }

    protected override IViewModel<TModelType> Model => RootModel ?? throw new InvalidOperationException("Rood Model not Found");
}