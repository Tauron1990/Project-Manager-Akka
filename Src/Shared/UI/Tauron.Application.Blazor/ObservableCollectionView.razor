@inherits NonAutoRenderingComponent
@typeparam TItem

@if(ListRender is null || EmptyRender is null)
{
    <MudText>Kein List Renderer angegeben</MudText>
}
else if(NonNullSource.Any())
{
    @ListRender(NonNullSource)
}
else if(EmptyRender is null)
{
    @ListRender(NonNullSource)
}
else
{
    @EmptyRender
}