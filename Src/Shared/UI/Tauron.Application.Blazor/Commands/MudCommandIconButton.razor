@inherits CommandContainer

<CascadingValue Value="CommandParameter" Name="@(nameof(CommandParameter))">
    <CascadingValue Value="Command" Name="@(nameof(Command))">
        <CommandHelper>
            <MudIconButton Disabled="@context.Disabled" OnClick="context.Execute" Icon="@Icon">
                @ChildContent
            </MudIconButton>
        </CommandHelper>
    </CascadingValue>
</CascadingValue>

@code {
    [Parameter]
    public RenderFragment? ChildContent { get; set; }

    [Parameter]
    public string? Icon { get; set; }

}
