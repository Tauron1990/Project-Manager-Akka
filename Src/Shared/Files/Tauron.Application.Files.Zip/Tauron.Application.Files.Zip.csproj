        <Compile Remove="Old\InZipDirectory.cs" />
        <None Include="Old\InZipDirectory.cs" />
        <Compile Remove="Old\InZipFile.cs" />
        <None Include="Old\InZipFile.cs" />
        <Compile Remove="Old\InZipFileSystem.cs" />
