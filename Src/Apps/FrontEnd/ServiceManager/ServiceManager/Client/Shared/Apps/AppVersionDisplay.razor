@using ServiceManager.Client.ViewModels.Apps

@implements GridShared.Columns.ICustomGridComponent<LocalAppInfo>

@if (Item.LastVersion == -1)
{
    <MudText>Keine Version Verfügbar</MudText>
}
else
{
    <MudText>@Item.LastVersion</MudText>
}

@code {

    [Parameter]
    public LocalAppInfo Item { get; set; } = LocalAppInfo.Empty;

}